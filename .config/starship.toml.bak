"$schema" = 'https://starship.rs/config-schema.json'
## FIRST LINE/ROW: Info & Status

format="$shell$all"
right_format="$time"
# First param â”€â”Œ


[shell]
format = "[â•­â”€$indicator]($style)"
unknown_indicator = "mystery shell"
style = "cyan bold"
disabled = false

[username]
format = " [$user]($style) "
style_user = "bold red"
style_root = "bold red"
show_always = true

# Second param
# [hostname]
# format = "[$hostname]($style) in "
# style = "bold dimmed red"
# trim_at = "-"
# ssh_only = false
# disabled = false

# Third param
[directory]
style = "purple"
truncation_length = 0
truncate_to_repo = true
truncation_symbol = "repo: "

# Before all the version info (python, nodejs, php, etc.)
[git_status]
style = "white"
ahead = "â‡¡${count}"
diverged = "â‡•â‡¡${ahead_count}â‡£${behind_count}"
behind = "â‡£${count}"
deleted = "x"

# Last param in the first line/row
[cmd_duration]
min_time = 1
format = "took [$duration]($style)"
disabled = false


## SECOND LINE/ROW: Prompt

# Prompt: optional param 1
[time]
format = " ðŸ•™ $time($style)\n"
time_format = "%T"
style = "bright-white"
disabled = true

# Prompt: param 2 â””â”€
[character]
success_symbol = "[â•°â”€Î»](cyan bold)"
error_symbol = "[â•°â”€ðŸ”´](cyan bold)"

# SYMBOLS
[status]
symbol = "ðŸ”´"
format = '[\[$symbol$status_common_meaning$status_signal_name$status_maybe_int\]]($style)'
map_symbol = true
disabled = true


[python]
format = '[${symbol}${pyenv_prefix}(${version} )(\($virtualenv\) )]($style)'

[java]
format = "[${symbol}(${version} )]($style)"

[aws]
disabled = true
format = 'on [$symbol($profile )(\($region\) )]($style)'
style = "bold blue"
[aws.region_aliases]
ap-southeast-2 = "au"
us-east-1 = "va"

[terraform]
format = "[$symbol$workspace]($style) "

[package]
disabled = true

